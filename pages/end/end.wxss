.container{
    display: flex;
    flex-direction:column;
    align-items: center;
    box-sizing: border-box;
    padding: 0 20rpx;
}
.avatar-box{
      margin-top:160rpx;
      width:200rpx;
    height:200rpx;
    border-radius: 50%;
    overflow: hidden
}
.avatar{
    width:200rpx;
    height:200rpx;

    border-radius: 50%;
}

.motto{
    margin-top:50rpx;
    font-size:32rpx;
    font-weight: bold;
}

.journey-container{
    margin-top: 100rpx;
    border: 1px solid #405f80;
    width: 200rpx;
    height: 80rpx;
    border-radius: 5px;
    text-align:center;
}

.journey{
    font-size:22rpx;
    font-weight: bold;
    line-height:80rpx;
    color: #405f80;
}

page{
    height: 100%;
}

.image-bg{
  top: 0;
  left: 0;
  z-index: -1;
  position: absolute;
  width: 100%;
  height: 100%;
}

.in-end{
  position: fixed;
  bottom: 50rpx;
}

.evaluate{
  text-align: center;
  font-size:26rpx;
    font-weight: bold;
    line-height:45rpx;
    color: #405f80;
    margin-top: 40rpx;
}